{
  "name": "perps-keepers",
  "version": "2.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "start:mainnet": "pm2 start ecosystem.config.js --only perps-keeper-mainnet",
    "start:goerli": "pm2 start ecosystem.config.js --only perps-keeper-goerli",
    "dev": "name=perps-keeper-goerli ts-node-dev --respawn --transpile-only src/index.ts run",
    "dev:mainnet": "name=perps-keeper-mainnet ts-node-dev --respawn --transpile-only src/index.ts run",
    "build": "tsc && touch .env && touch .env.staging && cp .env build/ && cp .env.staging build/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "upgrade-synthetix": "npm i @synthetixio/contracts-interface@latest @synthetixio/wei@latest synthetix@latest"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.231.0",
    "@aws-sdk/client-cloudwatch-logs": "3.72.0",
    "@ethersproject/abstract-provider": "5.5.1",
    "@ethersproject/experimental": "5.6.0",
    "@pythnetwork/pyth-evm-js": "^1.1.0",
    "@synthetixio/contracts-interface": "^2.77.3",
    "@synthetixio/wei": "^2.74.4",
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "lodash": "4.17.21",
    "log-process-errors": "6.3.0",
    "pm2": "5.1.2",
    "synthetix": "^2.88.1",
    "winston": "3.7.2",
    "winston-cloudwatch": "4.0.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.4",
    "@nomiclabs/hardhat-truffle5": "2.0.3",
    "@types/chai": "4.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.177",
    "@types/log-process-errors": "6.3.1",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.12",
    "chai": "4.3.6",
    "eslint": "7.6.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-havven": "1.1.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.0.0",
    "hardhat": "2.8.2",
    "jest": "27.4.3",
    "prettier": "1.19.1",
    "ts-jest": "27.1.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.2"
  }
}
